{
  "manifest_version": 2,
  "name": "Coding Train Pop Up",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
   "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/p5@1.1.4/lib/p5.min.js; object-src 'self'",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "ct.video",
    "default_popup": "sketch/index.html",
    "default_title": "Generate a mbappe dance battle!"
  }
}
